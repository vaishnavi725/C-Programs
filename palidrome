

#include <stdio.h>

int main()
{
    int n,num,rem,rev=0;
    scanf("%d",&n);
    num=n;
    
    while(n!=0)
    {
        rem=n%10;
        rev=rev*10+rem;
        n=n/10;
    }
     if(num<0)
    printf("invalid");
    else if(rev==num)
    printf("palidrome");
    else
    printf("not palidrome");

    return 0;
}
