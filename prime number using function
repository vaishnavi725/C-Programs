
#include <stdio.h>
#include<stdbool.h>
bool isprime(int n)
{
    int c=0;
    for(int i=1;i<=n;i++)
    {
        if(n%i==0)
        c++;
    }
    if(c==2)
    return true;
    else
    return false;
    
}

int main()
{
    int n,c=0,count=0;
    scanf("%d",&n);
    for(int i=1;i<=n;i++)
    {
    if(isprime(i))
    {
        printf("%d ",i);
        count++;
    }
    }
    printf("\nthe number prime number within n is=%d\n",count);
   
}
