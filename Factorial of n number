#include <stdio.h>


int fact(int n)
{
    int fact=1;
    for(int i=1;i<=n;i++)
    {
        fact=fact*i;
    }
    return fact;
}
int main() {
   int num,res;
   printf("enter a number:");
   scanf("%d",&num);
   if(num<0)
   {
       printf("enter valid number.");
   }
   else
   {
       printf("res=%d",fact(num));
   }
    return 0;
}
